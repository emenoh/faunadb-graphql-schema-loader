{
  "name": "faunadb-graphql-schema-loader",
  "version": "0.0.3",
  "description": "Package to simplify uploading GraphQL Schema to FaunaDB",
  "keywords": [
    "faunadb",
    "graphql",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "buildw": "tsc -w",
    "clean": "rimraf build/*",
    "lint": "eslint .",
    "postbuild": "prettier --write \"./build/**/*.{js,ts}\"",
    "prebuild": "npm run clean",
    "test": "jest --coverage",
    "testw": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "~25.2.1",
    "@types/node": "^14.0.23",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "~2.32.0",
    "@typescript-eslint/parser": "~2.32.0",
    "dotenv": "^8.2.0",
    "eslint": "~7.0.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-jest": "~23.10.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "~25.2.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "~25.5.1",
    "typescript": "~3.8.3"
  },
  "dependencies": {
    "@graphql-tools/schema": "^6.0.11",
    "got": "^11.5.0",
    "graphql": "^15.2.0"
  }
}